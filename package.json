{
  "name": "@effect-ak/effortless",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "description": "Code-first AWS Lambda framework",
  "files": [
    "dist"
  ],
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "effect",
    "typescript",
    "dynamodb",
    "api-gateway"
  ],
  "bin": {
    "eff": "./dist/cli/index.js",
    "effortless": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "postinstall": "gen-aws-sdk"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@aws-sdk/client-apigatewayv2": "^3.975.0",
    "@aws-sdk/client-dynamodb": "^3.975.0",
    "@aws-sdk/client-iam": "^3.975.0",
    "@aws-sdk/client-lambda": "^3.975.0",
    "@aws-sdk/client-resource-groups-tagging-api": "^3.975.0",
    "@aws-sdk/util-dynamodb": "^3.975.0",
    "@vercel/nft": "^1.3.0",
    "archiver": "^7.0.1",
    "esbuild": "^0.25.0",
    "glob": "^13.0.0",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@effect-ak/effortless-aws-gen": "workspace:*",
    "@effect/cli": "^0.73.1",
    "@effect/platform": "^0.94.2",
    "@effect/platform-node": "^0.104.1",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^7.0.0",
    "@types/aws-lambda": "^8.10.144",
    "@types/glob": "^9.0.0",
    "@types/node": "^22.10.1",
    "adm-zip": "^0.5.16",
    "effect": "^3.19.14",
    "tsup": "^8.5.1",
    "type-fest": "^4.22.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
