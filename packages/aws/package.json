{
  "name": "@effect-ak/effortless-aws",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "description": "AWS abstractions for effortless framework",
  "files": [
    "dist"
  ],
  "scripts": {
    "gen": "gen-aws-sdk",
    "prebuild": "pnpm gen",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./clients": {
      "types": "./dist/clients/index.d.ts",
      "default": "./dist/clients/index.js"
    }
  },
  "dependencies": {
    "@aws-sdk/client-apigatewayv2": "^3.975.0",
    "@aws-sdk/client-dynamodb": "^3.975.0",
    "@aws-sdk/client-iam": "^3.975.0",
    "@aws-sdk/client-lambda": "^3.975.0",
    "@aws-sdk/client-resource-groups-tagging-api": "^3.975.0",
    "@aws-sdk/util-dynamodb": "^3.975.0",
    "@vercel/nft": "^1.3.0",
    "archiver": "^7.0.1"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/node": "^22.10.1",
    "effect": "^3.19.14",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "@effect-ak/aws-sdk": "/Users/alexanderkondaurov/Projects/aws-sdk/dist/effect-ak-aws-sdk-1.0.0-rc.2.tgz"
  },
  "peerDependencies": {
    "effect": "^3.19.14"
  }
}
